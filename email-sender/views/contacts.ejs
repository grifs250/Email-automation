<html lang="en">
<head>
    <%- include('./partials/head') %>
    <title>Contacts</title>
    <link rel="stylesheet" href="/styles.css">
</head>
<body>
    <div class="container">
        <h1>All Contacts</h1>
        <a href="/contacts/new">Add New Contact</a>
        <ul>
            <% contacts.forEach(contact => { %>
                <li><%= contact.name %> - <%= contact.email %></li>
            <% }); %>
        </ul>
    <%- include('./partials/footer.ejs') %>
    </div>
</body>
</html>